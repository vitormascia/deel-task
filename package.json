{
  "name": "deel-task",
  "version": "0.1.0",
  "description": "This API shows off my skills!",
  "type": "module",
  "main": "./build/app/entry-points/app.entry-points.apiBoot.ts",
  "private": true,
  "scripts": {
    "seed": "ts-node ./scripts/seedDb.ts",
    "start": "ts-node ./src/app/entry-points/app.entry-points.apiBoot.ts",
    "api:boot:ts": "ts-node ./src/app/entry-points/app.entry-points.apiBoot.ts",
    "api:boot:js": "node ./build/app/entry-points/app.entry-points.apiBoot.js",
    "api:boot-watch:ts": "nodemon --watch ./src/**/*.ts --exec ts-node ./src/app/entry-points/app.entry-points.apiBoot.ts",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix && eslint --fix",
    "ts:compile": "rimraf build && tsc --pretty",
    "ts:compile-watch": "rimraf build && tsc --watch --pretty",
    "ts:check": "tsc --noEmit --pretty",
    "reinstall-modules": "rimraf node_modules && npm ci"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "sequelize": "^6.28.0",
    "snakecase-keys": "^5.4.5",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "4.9.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2"
  },
  "engines": {
    "node": ">=19.5.0"
  },
  "engineStrict": true
}
